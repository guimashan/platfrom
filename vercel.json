{
  "buildCommand": null,
  "outputDirectory": "public",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/liff/:path*",
      "destination": "/liff/:path*"
    },
    {
      "source": "/checkin/:path*",
      "destination": "/checkin/:path*"
    },
    {
      "source": "/service/:path*",
      "destination": "/service/:path*"
    },
    {
      "source": "/schedule/:path*",
      "destination": "/schedule/:path*"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
